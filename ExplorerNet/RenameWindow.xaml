<Window x:Class="ExplorerNet.RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStartupLocation="CenterScreen"
        Title="RenameWindow" Height="135" Width="300">
    <Grid Height="90">
        <TextBox Name="txtName" Margin="0,24,0,0" Height="29" VerticalAlignment="Top"></TextBox>
        <Button Name="btnOk" IsDefault="True" Margin="0,59,160,0" Height="24" VerticalAlignment="Top" Click="btnOk_Click">Ok</Button>
        <Button Name="btnCancel" IsCancel="True" Margin="132,59,28,0" Height="24" VerticalAlignment="Top">Cancel</Button>
    </Grid>
</Window>
